            "multiSelect": true,
            "quote": "'",
            "delimiter": ",",
            "query": "let AppInventory = AppInventory_CL;\r\nAppInventory\r\n| where AppName_s <> \"\"\r\n| distinct AppName_s\r\n| sort by AppName_s asc",
            "value": [
              "value::all"
